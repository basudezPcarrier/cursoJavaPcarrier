<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	      version="3.0">
  <display-name>CookiesYOtros</display-name>

<servlet>
	<servlet-name>Cookies</servlet-name>
	<servlet-class>com.wpsnetwork.servlets.Cookies</servlet-class>
</servlet>

<servlet>
	<servlet-name>ModificaCookies</servlet-name>
	<servlet-class>com.wpsnetwork.servlets.ModificaCookies</servlet-class>
</servlet>
<servlet>
	<description>
	</description>
	<display-name>Principal</display-name>
	<servlet-name>Principal</servlet-name>
	<servlet-class>com.wpsnetwork.servlets.Principal</servlet-class>
</servlet>
<servlet>
	<description>
	</description>
	<display-name>ReceptorDatos</display-name>
	<servlet-name>ReceptorDatos</servlet-name>
	<servlet-class>com.wpsnetwork.servlets.ReceptorDatos</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>Cookies</servlet-name>
	<url-pattern>/Cookies</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>ModificaCookies</servlet-name>
	<url-pattern>/modificaCookies</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Principal</servlet-name>
	<url-pattern>/index.html</url-pattern>
</servlet-mapping>




</web-app>
