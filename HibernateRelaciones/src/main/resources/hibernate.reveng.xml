<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC 
           "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" 
           "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

  <type-mapping>
    <sql-type jdbc-type="DATE" hibernate-type="java.time.LocalDate" length="7" not-null="true">
    </sql-type>
  </type-mapping>

  <table-filter match-name="PERSONAS"/>
  <table-filter match-name="DIRECCIONES"/>
</hibernate-reverse-engineering>